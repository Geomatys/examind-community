    "outputTransmission": ["reference", "value"],
    "inputs": [{
            "id": "urn:exa:wps:examind::test.echo:input:literal.input",
            "title": "Literal.input",
            "description": "Literal input.",
            "minOccurs": "0",
            "maxOccurs": "1",
            "input": {
                "literalDataDomains": [{
                        "anyValue": true,
                        "dataType": {
                            "name": "String",
                            "reference": "http://www.w3.org/TR/xmlschema-2/#string"
                        }
                    }]
            }
        }, {
            "id": "urn:exa:wps:examind::test.echo:input:boundingbox.input",
            "title": "Boundingbox.input",
            "description": "Boundingbox input.",
            "minOccurs": "0",
            "maxOccurs": "1",
            "input": {
                "supportedCRS": [{
                        "crs": "EPSG:4326",
                        "default": true
                    }, {
                        "crs": "CRS:84",
                        "default": false
                    }, {
                        "crs": "EPSG:3395",
                        "default": false
                    }, {
